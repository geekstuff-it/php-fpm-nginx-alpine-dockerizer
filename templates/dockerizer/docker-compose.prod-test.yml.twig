{% block content %}
version: '3.7'
services:
  php:
    build:
      context: ./
      target: prod
    privileged: false
    logging:
      driver: "none"
  nginx:
    build:
      context: ./
      target: nginx
    privileged: false
    environment:
      - PHP_FPM_HOST=php
    ports: ["${LOCAL_HTTP_PORT-8080}:8080"]
{% endblock %}
